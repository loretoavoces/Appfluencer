<div class="container">
    <h3>Contacto</h3>

    <div class="row justify-content-center align-items-center">

        <div class="col-9">
            <figure id="agencyMap"></figure>
        </div>
        <div class="col-3">
            {{#each allAgencies}}
            <ul>
                <strong>
                    <h6><a href="/influencer/mapa/{{id}}">{{name}}</a></h6>
                </strong>
                <p>{{street}}</p>
            </ul>
            {{/each}}

        </div>


        {{!-- Scripts est√°ticos --}}
        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
        <script src="/javascripts/styles.js"></script>
        <script src="/javascripts/agency-map.js"></script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBItnPEnmNORPYuMbfQHxK_lD6RHlgl5eg&callback=initApp"></script>